
    <main>
      <h1>{{car.name}}</h1>
      <img class="cube" src={{car.imageUrl}}>
      <div class="details">
        <p><span>Description:</span>{{car.description}}</p>
        <p><span>{{car.price}}</span></p>
        <a class="btn" href="/">Back</a>
        <a class="btn" href="/delete/{{car._id}}">Delete</a>
        <a class="btn" href="/edit/{{car._id}}">Edit</a>
      </div>
    </main>
